{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine":"auto",
        "jsxSingleQuote": true,
        "printWidth": 85,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "useTabs": false
      }
    ]
  }
}
